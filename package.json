{
  "name": "kluck.me",
  "private": true,
  "scripts": {
    "serve": "parcel serve 'src/**/!(_)*.{html,pug}'",
    "build": "rm -rf dist && parcel build --no-source-maps 'src/**/!(_)*.{html,pug}' && cp src/{.nojekyll,CNAME} dist",
    "deploy": "npm run build && gh-pages -d dist",
    "format": "prettier --write '**/*.{css,less,scss,graphql,gql,html,js,jsx,json,md,markdown,mdown,mkdn,mdx,ts,tsx,vue,yaml,yml}'"
  },
  "dependencies": {
    "promise-worker": "2.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "reactstrap": "8.5.1"
  },
  "devDependencies": {
    "autoprefixer": "9.8.6",
    "gh-pages": "3.1.0",
    "parcel-bundler": "1.12.4",
    "prettier": "2.0.5",
    "pug": "3.0.0",
    "sass": "1.26.10",
    "typescript": "3.9.7"
  }
}
