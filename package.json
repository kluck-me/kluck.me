{
  "name": "kluck.me",
  "private": true,
  "scripts": {
    "serve": "parcel serve 'src/**/!(_)*.pug'",
    "build": "rm -rf dist && parcel build --no-source-maps 'src/**/!(_)*.pug' && cp src/{.nojekyll,CNAME} dist",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "devDependencies": {
    "coffeescript": "^2.3.2",
    "parcel-bundler": "^1.11.0",
    "pug": "^2.0.3",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "gh-pages": "^2.0.1"
  }
}
