{
  "name": "kluck.me",
  "private": true,
  "scripts": {
    "serve": "parcel serve 'src/**/!(_)*.{html,pug}'",
    "build": "rm -rf dist && parcel build --no-source-maps 'src/**/!(_)*.{html,pug}' && cp src/{.nojekyll,CNAME} dist",
    "deploy": "npm run build && gh-pages -d dist",
    "format": "prettier --write '**/*.{css,less,scss,graphql,gql,html,js,jsx,json,md,markdown,mdown,mkdn,mdx,ts,tsx,vue,yaml,yml}'"
  },
  "devDependencies": {
    "autoprefixer": "9.7.4",
    "coffeescript": "2.5.1",
    "gh-pages": "2.2.0",
    "parcel-bundler": "1.12.4",
    "prettier": "1.19.1",
    "pug": "2.0.4",
    "sass": "1.26.3",
    "stylus": "0.54.7",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "promise-worker": "2.0.1",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "reactstrap": "8.4.1"
  }
}
